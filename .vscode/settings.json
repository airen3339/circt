{
    "files.associations": {
        "*.inc": "cpp",
        "__locale": "cpp",
        "functional": "cpp",
        "vector": "cpp",
        "__bit_reference": "cpp",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "__tree": "cpp",
        "array": "cpp",
        "bitset": "cpp",
        "deque": "cpp",
        "filesystem": "cpp",
        "forward_list": "cpp",
        "initializer_list": "cpp",
        "iterator": "cpp",
        "list": "cpp",
        "map": "cpp",
        "regex": "cpp",
        "set": "cpp",
        "span": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "hashtable": "cpp",
        "hash_map": "cpp",
        "hash_set": "cpp",
        "algorithm": "cpp"
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/llvm/**": true,
        "**/build/**": true,
        "**/include/**": false,
        "**/build/include/**": false,
        "**/llvm/mlir/include/**": false,
        "**/llvm/llvm/include/**": false,
        "**/llvm/build/include/**": false,
        "**/llvm/build/tools/mlir/include/**": false
    },
    "cmake.configureOnOpen": false,
    "C_Cpp.errorSquiggles": "Enabled",
    "C_Cpp.dimInactiveRegions": false,
    "editor.rulers": [80]
}